                              ItemsSource="{Binding Mode=OneWay, Source={StaticResource DataFromWhichCompare}}" 
                              SelectedItem="{Binding WhichCompare,Mode=TwoWay}" 
                              HorizontalAlignment="Left" 
                              Margin="0,3,0,0"/>
                    <DatePicker Width="100" SelectedDate="{Binding FilterDateTime,Mode=TwoWay}" Margin="0,3,0,0"/>
                    <DatePicker Width="100" 
                                DisplayDateStart="{Binding EndFilterStartDate,Mode=OneWay}" 
                                SelectedDate="{Binding FilterEndDate,Mode=TwoWay}" Margin="0,3,0,0"/>
                    <ToggleButton x:Name="DateToggle" 
                                  Width="32" 
                                  Height="32"
                                  Tag="Date"
                                  ToolTip="Date Filter"
                                  Command="{Binding ToggleFilterCommand,Mode=OneWay}">
                        <ToggleButton.CommandParameter>
                            <MultiBinding Converter="{StaticResource MultiValueToTuple}">
                                <Binding Path="IsChecked" ElementName="DateToggle"/>
                                <Binding Path="Tag" ElementName="DateToggle"/>
                            </MultiBinding>
                        </ToggleButton.CommandParameter>
                        <ToggleButton.Content>
                            <Image Source="/Filter.png" Width="16" Height="16"/>
                        </ToggleButton.Content></ToggleButton>
                   
                </StackPanel>
            </ToolBarPanel>
        </ToolBar>
