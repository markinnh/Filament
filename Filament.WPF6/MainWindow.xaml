<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:vm="clr-namespace:Filament.WPF6.ViewModels"        
        xmlns:local="clr-namespace:Filament.WPF6"
        xmlns:helpers1="clr-namespace:Filament.WPF6.Helpers" x:Class="Filament.WPF6.MainWindow"
        mc:Ignorable="d"
        Title="Filaments" Height="450" Width="800" SizeToContent="WidthAndHeight" x:Name="Main">
    <Window.Resources>
        <BooleanToVisibilityConverter x:Key="BooleanToVisibility"/>
        <helpers1:MultiValueToTuple x:Key="MultiValueToTuple"/>
        <Style x:Key="HeaderTextBlockStyle" TargetType="{x:Type TextBlock}">
            <Setter Property="Margin" Value="0,0,4,0"/>
        </Style>
        <StackPanel x:Key="InventoryHeader" Orientation="Horizontal">
            <TextBlock Text="Inventory" Style="{StaticResource HeaderTextBlockStyle}"/>
            <!--<Image Source="/shelf.png" Style="{StaticResource GlyphStyle}" />-->
            <Path Stroke="Black" StrokeThickness="1" Width="16" Height="16" Data="{StaticResource ShelfGlyph}" />
        </StackPanel>
    </Window.Resources>
    <Window.DataContext>
        <vm:MainWindowViewModel/>
    </Window.DataContext>
    <DockPanel>
        <Menu DockPanel.Dock="Top">
            <MenuItem Header="Page">
                <MenuItem Header="Inventory" IsCheckable="False">
                    <MenuItem x:Name="IntuitivePage" Header="Intuitive" Tag="Pages\ByVendorPage.xaml" Click="SelectPage_Click"/>
                    <MenuItem x:Name="SimplePage" Header="Simple" Tag="Pages\SimpleInventoryPage.xaml" Click="SelectPage_Click"/>
                </MenuItem>
                <Separator/>
                <MenuItem Header="Define" IsCheckable="False">
                    <MenuItem x:Name="FilamentPage" Header="Filament" Tag="Pages\FilamentDefnPage.xaml" Click="SelectPage_Click" />
                    <MenuItem x:Name="VendorPage" Header="Vendor" Tag="Pages\VendorDefnPage.xaml" Click="SelectPage_Click"/>
                    <MenuItem x:Name="PrintSettingPage" Header="Print Setting" Tag="Pages\DefinePrintSettingsPage.xaml" Click="SelectPage_Click"/>
                </MenuItem>
                <MenuItem x:Name="PrintSettingByVendorPage" Header="Print Settings by Vendor" Tag="Pages\PrintSettingsByVendor.xaml" Click="SelectPage_Click"/>
                <MenuItem x:Name="NotePage" Header="Notes" Tag="Pages\NotePage.xaml" Click="SelectPage_Click"/>
                <MenuItem x:Name="SettingsPage"  Header="Settings" Tag="Pages\SettingsPage.xaml" Click="SelectPage_Click"/>
            </MenuItem>
            <MenuItem Header="Filter">
                <!--<MenuItem Header="Tag" 
                          IsCheckable="True" 
                          IsChecked="{Binding TagMenuChecked,Mode=TwoWay}"/>
                <MenuItem Header="Keyword" 
                          IsCheckable="True" 
                          IsChecked="{Binding KeywordMenuChecked,Mode=TwoWay}"/>-->
                <MenuItem Header="Show">
                    <MenuItem x:Name="ShowAll" IsCheckable="True"  Header="All" Click="ShowState_Click"/>
                    <MenuItem x:Name="ShowInUse" IsCheckable="True" Header="In Use" Click="ShowState_Click"/>
                </MenuItem>
                
            </MenuItem>
            <MenuItem Header="Edit" Visibility="{Binding HasEdits,Converter={StaticResource BooleanToVisibility}}" />
        </Menu>
        <ToolBar DockPanel.Dock="Left">
            <ToolBarPanel Orientation="Vertical">
                <!--<ComboBox Name="SelectShowFlag"
                          Width="100"  
                          SelectionChanged="ComboBox_SelectionChanged"
                          ItemsSource="{Binding Mode=OneWay, Source={StaticResource DataFromShowAllFlag}}">
                    <ComboBox.ItemTemplate>
                        <DataTemplate>
                            <TextBlock>
                        <Run Text="{Binding . ,Converter={StaticResource  EnumDescriptionConverter},Mode=OneWay}"/>
